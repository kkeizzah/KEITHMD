    "PREFIX": {
      "description": "Bot command prefix",
      "value": ".",
      "required": false
    },
    "AUTHOR": {
      "description": "Bot author name",
      "value": "Keith",
      "required": false
    },
    "BOT_PIC": {
      "description": "Bot profile picture URL",
      "value": "https://files.catbox.moe/9zqj7g.jpg",
      "required": false
    },
    "BOTNAME": {
      "description": "Bot display name",
      "value": "KEITH-MD",
