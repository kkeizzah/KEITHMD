{
  "name": "KEITH-MD",
  "description": "This is a simple whatsapp bot built with nodejs to make your WhatsApp enjoyable",
  "keywords": [
    "bot",
    "whatsapp",
    "keith",
    "whatsapp-stickers",
    "anime",
    "whatsapp-bot",
    "whatsapp-sticker-bot",
    "whatsapp-botto",
    "baileys",
    "baileys-md",
    "md-bot"
  ],
  "website": "https://github.com/kkeizzah/KEITH-MD#readme",
  "repository": "https://github.com/kkeizzah/KEITH-MD.git",
  "logo": "https://i.imgur.com/dAtHp2k.jpeg",
  "success_url": "/",
  "env": {
    "OWNER_NUMBER": {
      "description": "Put any phone numbers you want to be bot owner without + or spaces \",\" Ex: 254748387615, 254796299159",
      "required": false
    },
    "SESSION": {
      "description": "Add your session id here",
      "required": true
    },
    "DATABASE_URL": {
      "description": "Database URL (leave as default for local SQLite)",
      "value": "./database.db",
      "required": false
    },
    "ANTICALL_STATUS": {
      "description": "Enable/disable anti-call feature (true/false)",
      "value": "false",
      "required": false
    },
    "ANTICALL_MESSAGE": {
      "description": "Message to send when rejecting calls",
      "value": "Call me later üôè",
      "required": false
    },
    "ANTICALL_ACTION": {
      "description": "Action to take on calls (reject/block)",
      "value": "reject",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
    }
  ],
  "stack": "heroku-24"
}
