{
  "name": "KEITH-MD",
  "description": "This is a simple whatsapp bot built with nodejs to make your WhatsApp enjoyable",
  "keywords": [
    "bot",
    "whatsapp",
    "keith",
    "whatsapp-stickers",
    "anime",
    "whatsapp-bot",
    "whatsapp-sticker-bot",
    "whatsapp-botto",
    "baileys",
    "baileys-md",
    "md-bot"
  ],
  "website": "https://github.com/kkeizzah/KEITH-MD#readme",
  "repository": "https://github.com/kkeizzah/KEITH-MD.git",
  "logo": "https://i.imgur.com/dAtHp2k.jpeg",
  "success_url": "/",
  "env": {
    "OWNER_NUMBER": {
      "description": "Put any phone numbers you want to be bot owner without + or spaces \",\" Ex: 254748387615, 254796299159",
      "required": false
    },
    "SESSION": {
      "description": "Add your session id here",
      "required": true
    },
    "DATABASE_URL": {
      "description": "Database URL (leave as default for local SQLite)",
      "value": "./database.db",
      "required": false
    },
    
    // AntiCall settings
    "ANTICALL_STATUS": {
      "description": "Enable/disable anti-call feature (true/false)",
      "value": "false",
      "required": false
    },
    "ANTICALL_MESSAGE": {
      "description": "Message to send when rejecting calls",
      "value": "Call me later üôè",
      "required": false
    },
    "ANTICALL_ACTION": {
      "description": "Action to take on calls (reject/block)",
      "value": "reject",
      "required": false
    },
    
    // AntiDelete settings
    "ANTIDELETE_STATUS": {
      "description": "Enable/disable anti-delete feature (true/false)",
      "value": "true",
      "required": false
    },
    "ANTIDELETE_NOTIFICATION": {
      "description": "Notification text for deleted messages",
      "value": " *Keith antiDelete*",
      "required": false
    },
    "ANTIDELETE_INCLUDE_GROUP_INFO": {
      "description": "Include group info in anti-delete notifications (true/false)",
      "value": "true",
      "required": false
    },
    "ANTIDELETE_SEND_TO_OWNER": {
      "description": "Send deleted messages to bot owner (true/false)",
      "value": "true",
      "required": false
    },
    "ANTIDELETE_INCLUDE_MEDIA": {
      "description": "Include media in anti-delete notifications (true/false)",
      "value": "true",
      "required": false
    },
    
    // AntiLink settings
    "ANTILINK_STATUS": {
      "description": "Anti-link mode: off, warn, delete, or remove",
      "value": "off",
      "required": false
    },
    "ANTILINK_ACTION": {
      "description": "Action for link violations: warn, delete, or remove",
      "value": "warn",
      "required": false
    },
    "ANTILINK_WARN_LIMIT": {
      "description": "Number of warnings before taking action",
      "value": "3",
      "required": false
    },
    
    // AntiStatusMention settings
    "ANTISTATUSMENTION_STATUS": {
      "description": "Anti-status-mention mode: off, warn, delete, or remove",
      "value": "warn",
      "required": false
    },
    "ANTISTATUSMENTION_ACTION": {
      "description": "Action for status mention violations: warn, delete, or remove",
      "value": "warn",
      "required": false
    },
    "ANTISTATUSMENTION_WARN_LIMIT": {
      "description": "Number of warnings before taking action",
      "value": "3",
      "required": false
    },
    
    // AutoBio settings
    "AUTOBIO_STATUS": {
      "description": "Enable auto bio: on or off",
      "value": "off",
      "required": false
    },
    "AUTOBIO_MESSAGE": {
      "description": "Auto bio message",
      "value": "KEITH-MD Always active!",
      "required": false
    },
    
    // AutoRead settings
    "AUTOREAD_STATUS": {
      "description": "Enable auto read messages (true/false)",
      "value": "false",
      "required": false
    },
    "AUTOREAD_CHAT_TYPES": {
      "description": "Chat types for auto read: private,group",
      "value": "private,group",
      "required": false
    },
    
    // AutoStatus settings
    "AUTOSTATUS_AUTOVIEW_STATUS": {
      "description": "Auto view status (true/false)",
      "value": "true",
      "required": false
    },
    "AUTOSTATUS_AUTOLIKE_STATUS": {
      "description": "Auto like status (true/false)",
      "value": "false",
      "required": false
    },
    "AUTOSTATUS_AUTOREPLY_STATUS": {
      "description": "Auto reply to status (true/false)",
      "value": "false",
      "required": false
    },
    "AUTOSTATUS_STATUS_REPLY_TEXT": {
      "description": "Text to reply to status with",
      "value": "‚úÖ Status Viewed By Keith Md",
      "required": false
    },
    "AUTOSTATUS_STATUS_LIKE_EMOJIS": {
      "description": "Emojis to use for liking status",
      "value": "üíõ,‚ù§Ô∏è,üíú,ü§ç,üíô",
      "required": false
    },
    
    // Chatbot settings
    "CHATBOT_STATUS": {
      "description": "Enable chatbot: on or off",
      "value": "off",
      "required": false
    },
    "CHATBOT_MODE": {
      "description": "Chatbot mode: private, group, or both",
      "value": "private",
      "required": false
    },
    "CHATBOT_TRIGGER": {
      "description": "Chatbot trigger word",
      "value": "dm",
      "required": false
    },
    "CHATBOT_DEFAULT_RESPONSE": {
      "description": "Default response type: text or audio",
      "value": "text",
      "required": false
    },
    "CHATBOT_VOICE": {
      "description": "Voice for audio responses",
      "value": "Kimberly",
      "required": false
    },
    
    // Greet settings
    "GREET_ENABLED": {
      "description": "Enable greet messages (true/false)",
      "value": "false",
      "required": false
    },
    "GREET_MESSAGE": {
      "description": "Greet message template",
      "value": "Hello @user üëã\\nWelcome to my chat!\\nHow can I help you today?",
      "required": false
    },
    
    // GroupEvents settings
    "GROUPEVENTS_ENABLED": {
      "description": "Enable group events (true/false)",
      "value": "false",
      "required": false
    },
    "GROUPEVENTS_WELCOME_MESSAGE": {
      "description": "Welcome message for new members",
      "value": "Hey @user üëã\\nWelcome to *{group}*.\\nYou're member #{count}.\\nTime: *{time}*\\nDescription: {desc}",
      "required": false
    },
    "GROUPEVENTS_GOODBYE_MESSAGE": {
      "description": "Goodbye message for leaving members",
      "value": "Goodbye @user üòî\\nLeft at: *{time}*\\nMembers left: {count}",
      "required": false
    },
    "GROUPEVENTS_SHOW_PROMOTIONS": {
      "description": "Show promotion/demotion messages (true/false)",
      "value": "true",
      "required": false
    },
    
    // Presence settings
    "PRESENCE_PRIVATE_CHAT": {
      "description": "Presence in private chat: off, online, typing, or recording",
      "value": "off",
      "required": false
    },
    "PRESENCE_GROUP_CHAT": {
      "description": "Presence in group chat: off, online, typing, or recording",
      "value": "off",
      "required": false
    },
    
    // Bot settings
    "BOT_PREFIX": {
      "description": "Bot command prefix",
      "value": ".",
      "required": false
    },
    "BOT_AUTHOR": {
      "description": "Bot author name",
      "value": "Keith",
      "required": false
    },
    "BOT_URL": {
      "description": "Bot image URL",
      "value": "https://files.catbox.moe/9zqj7g.jpg",
      "required": false
    },
    "BOT_GURL": {
      "description": "Bot GitHub URL",
      "value": "https://github.com/Keithkeizzah/KEITH-MD",
      "required": false
    },
    "BOT_TIMEZONE": {
      "description": "Bot timezone",
      "value": "Africa/Nairobi",
      "required": false
    },
    "BOT_BOTNAME": {
      "description": "Bot name",
      "value": "KEITH-MD",
      "required": false
    },
    "BOT_PACKNAME": {
      "description": "Sticker pack name",
      "value": "KEITH-MD",
      "required": false
    },
    "BOT_MODE": {
      "description": "Bot mode: public or private",
      "value": "public",
      "required": false
    },
    "BOT_SESSION_NAME": {
      "description": "Bot session name",
      "value": "keith-md",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
    }
  ],
  "stack": "heroku-24"
}
